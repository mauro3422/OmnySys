{
  "llm": {
    "enabled": true,
    "gpu": {
      "port": 8000,
      "parallel": 4
    },
    "cpu": {
      "port": 8001,
      "parallel": 2
    }
  },
  "analysis": {
    "useStaticFirst": true,
    "llmOnlyForComplex": true,
    "complexityThreshold": 0.7,
    "confidenceThreshold": 0.8
  },
  "performance": {
    "enableCPUFallback": false,
    "maxConcurrentAnalyses": 4,
    "timeout": 30000
  }
}