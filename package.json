{
  "name": "omnysys",
  "version": "0.9.37",
  "description": "Motor de Contexto Multi-Capa para IAs - Previene visión de túnel al editar código",
  "main": "omny.js",
  "bin": {
    "omny": "omny.js",
    "omnysys": "omny.js"
  },
  "type": "module",
  "scripts": {
    "install:all": "node install.js",
    "start": "node omny.js up",
    "stop": "node omny.js down",
    "status": "node omny.js status",
    "tools": "node omny.js tools",
    "setup": "node omny.js setup",
    "analyze": "node src/cli/index.js analyze",
    "dev": "node omny.js up",
    "smoke": "node tests/smoke-test.js",
    "test": "vitest run",
    "test:unit": "vitest run --config tests/config/vitest.unit.config.js",
    "test:integration": "vitest run --config tests/config/vitest.integration.config.js",
    "test:integration:layer-a": "vitest run tests/integration/layer-a --config tests/config/vitest.integration.config.js",
    "test:integration:layer-b": "vitest run tests/integration/layer-b --config tests/config/vitest.integration.config.js",
    "test:integration:cross-layer": "vitest run tests/integration/cross-layer --config tests/config/vitest.integration.config.js",
    "test:e2e": "vitest run --config tests/config/vitest.e2e.config.js",
    "test:contracts": "vitest run tests/contracts --config tests/config/vitest.unit.config.js",
    "test:contracts:layer-a": "vitest run tests/contracts/layer-a-extractor.contract.test.js --config tests/config/vitest.unit.config.js",
    "test:contracts:layer-b": "vitest run tests/contracts/layer-b-metadata.contract.test.js --config tests/config/vitest.unit.config.js",
    "test:contracts:cross-layer": "vitest run tests/contracts/cross-layer.contract.test.js --config tests/config/vitest.unit.config.js",
    "test:layer-a:core": "vitest run tests/unit/layer-a --config tests/config/vitest.unit.config.js",
    "test:layer-a:extractor:js": "vitest run tests/unit/layer-a/extractors/javascript --config tests/config/vitest.unit.config.js",
    "test:layer-b": "vitest run tests/unit/layer-b-semantic --config tests/config/vitest.unit.config.js",
    "test:layer-b:coverage": "vitest run tests/unit/layer-b-semantic --config tests/config/vitest.unit.config.js --coverage",
    "test:layer-b:validators": "vitest run tests/unit/layer-b-semantic/validators --config tests/config/vitest.unit.config.js",
    "test:layer-b:schema-validator": "vitest run tests/unit/layer-b-semantic/schema-validator --config tests/config/vitest.unit.config.js",
    "test:layer-b:metadata-contract": "vitest run tests/unit/layer-b-semantic/metadata-contract --config tests/config/vitest.unit.config.js",
    "test:layer-b:issue-detectors": "vitest run tests/unit/layer-b-semantic/issue-detectors --config tests/config/vitest.unit.config.js",
    "test:layer-b:project-analyzer": "vitest run tests/unit/layer-b-semantic/project-analyzer --config tests/config/vitest.unit.config.js",
    "test:layer-b:llm-analyzer": "vitest run tests/unit/layer-b-semantic/llm-analyzer --config tests/config/vitest.unit.config.js",
    "test:layer-b:prompt-engine": "vitest run tests/unit/layer-b-semantic/prompt-engine --config tests/config/vitest.unit.config.js",
    "test:layer-b:lineage-validator": "vitest run tests/unit/layer-b-semantic/validators/lineage-validator --config tests/config/vitest.unit.config.js",
    "test:layer-c": "vitest run tests/unit/layer-c --config tests/config/vitest.unit.config.js",
    "test:layer-c:shadow-registry": "vitest run tests/unit/layer-c/shadow-registry --config tests/config/vitest.unit.config.js",
    "test:layer-c:verification": "vitest run tests/unit/layer-c/verification --config tests/config/vitest.unit.config.js",
    "test:layer-c:mcp": "vitest run tests/unit/layer-c/mcp --config tests/config/vitest.unit.config.js",
    "test:layer-c:coverage": "vitest run tests/unit/layer-c --config tests/config/vitest.unit.config.js --coverage",
    "test:contracts:layer-c": "vitest run tests/contracts/layer-c --config tests/config/vitest.unit.config.js",
    "test:integration:layer-c": "vitest run tests/integration/layer-c --config tests/config/vitest.integration.config.js",
    "test:core": "vitest run tests/unit/core --config tests/config/vitest.unit.config.js",
    "test:core:atomic-editor": "vitest run tests/unit/core/atomic-editor --config tests/config/vitest.unit.config.js",
    "test:core:orchestrator": "vitest run tests/unit/core/orchestrator --config tests/config/vitest.unit.config.js",
    "test:core:unified-server": "vitest run tests/unit/core/unified-server --config tests/config/vitest.unit.config.js",
    "test:layer-graph": "vitest run tests/unit/layer-graph --config tests/config/vitest.unit.config.js",
    "test:layer-graph:contracts": "vitest run tests/contracts/layer-graph --config tests/config/vitest.unit.config.js",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:layer-a": "vitest run tests/unit/layer-a --config tests/config/vitest.unit.config.js --coverage",
    "test:coverage:layer-b": "vitest run tests/unit/layer-b-semantic --config tests/config/vitest.unit.config.js --coverage",
    "test:coverage:all": "vitest run tests/unit --config tests/config/vitest.unit.config.js --coverage",
    "test:performance": "vitest run tests/performance --config tests/config/vitest.integration.config.js",
    "build": "echo \"No build step needed for now\"",
    "lint": "echo \"No linter configured yet\"",
    "validate": "node scripts/validate-syntax.js",
    "check": "node scripts/validate-syntax.js && node scripts/detect-broken-imports.js",
    "postinstall": "node install.js",
    "mcp": "node --max-old-space-size=8192 src/layer-c-memory/mcp-server.js",
    "mcp:debug": "node --max-old-space-size=4096 --inspect src/layer-c-memory/mcp-server.js"
  },
  "keywords": [
    "ai",
    "code-analysis",
    "dependency-graph",
    "mcp",
    "static-analysis",
    "semantic-analysis",
    "developer-tools",
    "context-engine"
  ],
  "author": "mauro3422",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "imports": {
    "#ai/*": "./src/ai/*",
    "#cli/*": "./src/cli/*",
    "#config/*": "./src/config/*",
    "#core/*": "./src/core/*",
    "#layer-graph/*": "./src/layer-graph/*",
    "#layer-a/*": "./src/layer-a-static/*",
    "#layer-b/*": "./src/layer-b-semantic/*",
    "#layer-c/*": "./src/layer-c-memory/*",
    "#molecular-chains/*": "./src/layer-a-static/pipeline/molecular-chains/*",
    "#services/*": "./src/services/*",
    "#shared/*": "./src/shared/*",
    "#test-factories/*": "./tests/factories/*",
    "#utils/*": "./src/utils/*",
    "#validation/*": "./src/validation/*"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fast-glob": "^3.3.2",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.18",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.2.0",
    "ollama": "^0.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauro3422/OmnySys.git"
  },
  "bugs": {
    "url": "https://github.com/mauro3422/OmnySys/issues"
  },
  "homepage": "https://github.com/mauro3422/OmnySys#readme"
}
